#include <bits/stdc++.h>
using namespace std;

bool es_simetrico(const vector<int> & vector);

int main() {
    int n;
    cout << "Ingresa la longitud del vector: ";
    cin >> n;

    vector<int> vector(n);
    cout << "Ingresa los valores del vector uno por uno:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> vector[i];
    }

    if (es_simetrico(vector)) {
        cout << "El vector es simétrico." << endl;
    } else {
        cout << "El vector no es simétrico." << endl;
    }

    return 0;
}
bool es_simetrico(const vector<int> & vector) {
    int n = vector.size();
    for (int i = 0; i < n / 2; i++) {
        if (vector[i] != vector[n - 1 - i]) {
            return false;
        }
    }
    return true;
}
